/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b,out=Nand);
    Or(a=a,b=b,out=Or);
    And(a=Nand,b=Or,out=out);
}